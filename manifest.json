    "name": "Popup Timer",
    "version": "1.0",
    "description": "Display a small draggable timer on your browser.",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["jquery-3.5.1.min.js", "jquery-ui.js", "main.js"],
        "css": ["style.css"]
    }]
}